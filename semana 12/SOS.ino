#define led 2;

void setup()
{
  pinMode(led, OUTPUT);
}

void loop()
{
  digitalWrite(led, HIGH);
  delay(200); 
  digitalWrite(led, LOW);
  delay(200); 
  
  digitalWrite(led, HIGH);
  delay(200); 
  digitalWrite(led, LOW);
  delay(200);
  
   digitalWrite(led, HIGH);
  delay(200); 
  digitalWrite(led, LOW);
  delay(200);
  
   digitalWrite(led, HIGH);
  delay(500); 
  digitalWrite(led, LOW);
  delay(500);
  
   digitalWrite(led, HIGH);
  delay(500); 
  digitalWrite(led, LOW);
  delay(500);
  
   digitalWrite(led, HIGH);
  delay(500); 
  digitalWrite(led, LOW);
  delay(500);
  
  digitalWrite(led, HIGH);
  delay(200); 
  digitalWrite(led, LOW);
  delay(200); 
  
  digitalWrite(led, HIGH);
  delay(200); 
  digitalWrite(led, LOW);
  delay(200);
  
   digitalWrite(led, HIGH);
  delay(200); 
  digitalWrite(led, LOW);
  delay(1000);
} 